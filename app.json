{
  "expo": {
    "android": {
      "package": "com.ovftank.apk-update",
      "permissions": [
        "INTERNET",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "MANAGE_EXTERNAL_STORAGE",
        "REQUEST_INSTALL_PACKAGES",
        "INSTALL_PACKAGES",
        "DELETE_PACKAGES",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "SYSTEM_ALERT_WINDOW",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "BACKGROUND_TASK",
        "ACCESS_DOWNLOAD_MANAGER",
        "WRITE_SETTINGS"
      ]
    },
    "icon": "./assets/images/icon.png",
    "name": "APK UPDATE",
    "plugins": [
      "expo-router",
      "expo-background-task",
      "expo-task-manager",
      "expo-notifications",
      "expo-intent-launcher"
    ],
    "scheme": "apk-update",
    "slug": "apk-update",
    "version": "1.0.1"
  }
}